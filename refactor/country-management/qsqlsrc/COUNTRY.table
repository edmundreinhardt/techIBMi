-- %TEXT Country file

CREATE OR REPLACE TABLE COUNTRY (
  COID CHAR(2) NOT NULL COMMENT 'CO ID - COUNTRY CODE',
  COUNTR CHAR(30) NOT NULL COMMENT 'COUNTRY NAME',
  COISO CHAR(3) COMMENT 'ISO Country Code',
  PRIMARY KEY(COID)
);

LABEL ON TABLE COUNTRY IS 'Country File';
LABEL ON COLUMN COUNTRY.COID IS 'Country Code';
LABEL ON COLUMN COUNTRY.COUNTR IS 'Country Name';
LABEL ON COLUMN COUNTRY.COISO IS 'ISO Country Code';